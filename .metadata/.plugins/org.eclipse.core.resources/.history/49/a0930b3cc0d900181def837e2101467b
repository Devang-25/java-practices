package com.preety.chessboard.domain;

public class Piece {
	private Color color; 
	private PieceType piece;
	
	public void move(Position start, Position end) {
		System.out.println("move from " + start + " to end " + end);
	}

}

enum Color{
	W, //white
	B; //black
}

enum PieceType{
	R, //Rook
	B, //Bishop
	H, //Horse
	K, //King
	Q, // Queen
	P; //Pawn
}
